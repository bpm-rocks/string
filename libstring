        string=${string//Ã¿/Å¸}
        string=${string//Ä/Ä€}
        string=${string//Äƒ/Ä‚}
        string=${string//Ä…/Ä„}
        string=${string//Ä‡/Ä†}
        string=${string//Ä‰/Äˆ}
        string=${string//Ä‹/ÄŠ}
        string=${string//Ä/ÄŒ}
