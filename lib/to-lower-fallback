        string=${string//ğ¤Ÿ/ğ¥}
        string=${string//ğ¤ /ğ¥‚}
        string=${string//ğ¤¡/ğ¥ƒ}
    fi

    local "${1-}" && assign::value "$1" "$string"
}

if [[ "${BASH_VERSINFO[0]}" -lt 4 ]] || [[ "${BASH_VERSINFO[0]}" -eq 4 && "${BASH_VERSINFO[1]}" -lt 2 ]]; then
    #: Use the fallback because the faster method is not available.
    string::toLower() {
        string::toLowerFallback "$1" "$2"
    }
fi
