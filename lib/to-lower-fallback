        string=${string//𐓊/𐓲}
        string=${string//𐓋/𐓳}
        string=${string//𐓌/𐓴}
        string=${string//𐓍/𐓵}
        string=${string//𐓎/𐓶}
        string=${string//𐓏/𐓷}
        string=${string//𐓐/𐓸}
        string=${string//𐓑/𐓹}
        string=${string//𐓒/𐓺}
        string=${string//𐓓/𐓻}
        string=${string//𐲀/𐳀}
        string=${string//𐲁/𐳁}
        string=${string//𐲂/𐳂}
        string=${string//𐲃/𐳃}
        string=${string//𐲄/𐳄}
        string=${string//𐲅/𐳅}
        string=${string//𐲆/𐳆}
        string=${string//𐲇/𐳇}
        string=${string//𐲈/𐳈}
        string=${string//𐲉/𐳉}
        string=${string//𐲊/𐳊}
