        string=${string//Ã–/Ã¶}
        string=${string//Ã˜/Ã¸}
        string=${string//Ã™/Ã¹}
        string=${string//Ãš/Ãº}
        string=${string//Ã›/Ã»}
        string=${string//Ãœ/Ã¼}
        string=${string//Ã/Ã½}
        string=${string//Ã/Ã¾}
        string=${string//Ä€/Ä}
        string=${string//Ä‚/Äƒ}
        string=${string//Ä„/Ä…}
        string=${string//Ä†/Ä‡}
        string=${string//Äˆ/Ä‰}
        string=${string//ÄŠ/Ä‹}
        string=${string//ÄŒ/Ä}
        string=${string//Ä/Ä}
        string=${string//Ä/Ä‘}
        string=${string//Ä’/Ä“}
        string=${string//Ä”/Ä•}
        string=${string//Ä–/Ä—}
        string=${string//Ä˜/Ä™}
        string=${string//Äš/Ä›}
        string=${string//Äœ/Ä}
