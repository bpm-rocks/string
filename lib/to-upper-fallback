        string=${string//ø/Ø}
        string=${string//ù/Ù}
        string=${string//ú/Ú}
        string=${string//û/Û}
        string=${string//ü/Ü}
        string=${string//ý/Ý}
        string=${string//þ/Þ}
        string=${string//ÿ/Ÿ}
        string=${string//ā/Ā}
        string=${string//ă/Ă}
        string=${string//ą/Ą}
        string=${string//ć/Ć}
        string=${string//ĉ/Ĉ}
        string=${string//ċ/Ċ}
        string=${string//č/Č}
        string=${string//ď/Ď}
        string=${string//đ/Đ}
        string=${string//ē/Ē}
        string=${string//ĕ/Ĕ}
        string=${string//ė/Ė}
        string=${string//ę/Ę}
        string=${string//ě/Ě}
        string=${string//ĝ/Ĝ}
        string=${string//ğ/Ğ}
        string=${string//ġ/Ġ}
        string=${string//ģ/Ģ}
