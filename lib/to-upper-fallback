        string=${string//Ã¼/Ãœ}
        string=${string//Ã½/Ã}
        string=${string//Ã¾/Ã}
        string=${string//Ã¿/Å¸}
        string=${string//Ä/Ä€}
        string=${string//Äƒ/Ä‚}
        string=${string//Ä…/Ä„}
        string=${string//Ä‡/Ä†}
        string=${string//Ä‰/Äˆ}
        string=${string//Ä‹/ÄŠ}
